--- standard boot config obj
[router=>[
 resolve=>[
  namespace=>[:=>/mmadt/,fos:=>/fos/,math:=>/mmadt/ext/math],
  auto_prefix=>[<>,/mmadt/,/fos/,/sys/],
  query=>[write=>[lock=>to_do]],
  default_config=>[query=>[write=>[sub=>noobj]]]]],
 scheduler=>[
  def_stack_size=>8096],
 mqtt=>[
  broker=><mqtt://chibi.local:1883>,
  client=>fhatos_client],
 console=>[
  terminal=>[
   stdout=>/io/terminal/:stdout,
   stdin=>/io/terminal/:stdin
  ],
  nest=>2,
  prompt=>'!mfhatos!g>!! ',
  strict=>false,
  log=>INFO,
  stack_size=>24288
 ],
 fs=>[root=><disk>]
]@/boot/config