[router=>[
  resolve=>[
   namespace=>[:=>/mmadt/,fos:=>/fos/,math:=>/mmadt/ext/math],
   auto_prefix=>[<>,/mmadt/,/fos/,/sys/]]],
 scheduler=>[
  def_stack_size=>8096
 ],
 mqtt=>[
  broker=><mqtt://192.168.10.127:1883>,
  client=>fhatos_client],
 console=>[
  terminal=>[
   stdout=>/io/terminal/:stdout,
   stdin=>/io/terminal/:stdin
  ],
  nest=>2,
  prompt=>'!mfhatos!g>!! ',
  strict=>false,
  log=>INFO,
  stack_size=>24288
 ],
 log=>[
    ERROR=>[#],
    INFO=>[#],
    WARN=>[],
    DEBUG=>[],
    TRACE=>[]
 ],
 ota=>[
  host=>fhatos_ota,
  port=>3232
 ]
]@/sys/config