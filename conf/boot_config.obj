[router=>[
  resolve=>[
   namespace=>[:=>/mmadt/,fos:=>/fos/,math:=>/mmadt/ext/math],
   auto_prefix=>[<>,/mmadt/,/fos/,/sys/]]],
 mqtt=>[
  broker=><mqtt://192.168.10.127:1883>,
  client=>fhatos_client],
 console=>[
  terminal=>[
   stdout=>/io/terminal/:stdout,
   stdin=>/io/terminal/:stdin
  ],
  nest=>2,
  prompt=>'!mfhatos!y>!! ',
  strict=>false,
  log=>INFO
 ],
 log=>[
    ERROR=>[#],
    INFO=>[#],
    WARN=>[],
    DEBUG=>[],
    TRACE=>[]
 ]
]@/sys/config